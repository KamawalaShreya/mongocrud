<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>API Changelogs | Dating App</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="<%= logo %>"
    />
    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.bundle.min.js"></script>
    <style type="text/css">
      body {
        margin-top: 20px;
        font-size: 0.9rem;
      }

      .text-muted {
        color: #8492a6 !important;
      }

      .bg-light {
        background-color: #add8e624 !important;
      }

      ul {
        list-style-position: outside;
      }

      .text-success {
        color: #0f84ab !important;
      }
    </style>
  </head>

  <body data-new-gr-c-s-check-loaded="14.1050.0" data-gr-ext-installed="">
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/MaterialDesign-Webfont/5.3.45/css/materialdesignicons.min.css"
      integrity="sha256-nwNjrH7J9zS/Ti4twtWX7OsC5QdQHCIKTv5cLMsGo68="
      crossorigin="anonymous"
    />
    <div class="container">
      <div class="row justify-content-center">
        <h3 class="pt-2 pb-4 pl-3">Dating App Changelogs</h3>
        <hr />
        <br />
        <div>
          <img class="" src="<%= logo %>" alt="" />
        </div>
        <div class="col-lg-12 col-md-12 col-12 mx-auto my-auto">
          <div class="row d-flex">
            <div class="col-lg-2">
              <h5 class="mt-4"><b> 19th May 2023 </b></h5>
            </div>
            <div class="col-lg-10 saprater">
              <h6 class="mt-4">
                <span class="p-2 bg-light shadow rounded text-success"
                  >Version 1.0</span
                >
              </h6>
              <br>
              <span class="badge badge-success">New Additions</span>
              <ul>
                <li class="text-muted ml-3 mb-2">
                  <code>GET /users</code><br />
                  ✓ Get user profile data
                </li>
                <li class="text-muted ml-3 mb-2">
                  <code>POST /users/forgot-password</code><br />
                  ✓ Send forgot password otp email
                </li>
                <li class="text-muted ml-3 mb-2">
                  <code>POST /users/reset-password</code><br />
                  ✓ Reset Password after otp verify
                </li>
              </ul>
            </div>
          </div>
          <div class="row d-flex">
            <div class="col-lg-2">
              <h5 class="mt-4"><b> 18th May 2023 </b></h5>
            </div>
            <div class="col-lg-10 saprater">
              <h6 class="mt-4">
                <span class="p-2 bg-light shadow rounded text-success"
                  >Version 1.0</span
                >
              </h6>
              <br>
              <span class="badge badge-success">New Additions</span>
              <ul>
                <li class="text-muted ml-3 mb-2">
                  <code>POST /auth/register</code><br />
                  ✓ Register User
                </li>
                <li class="text-muted ml-3 mb-2">
                  <code>POST /auth/login</code><br />
                  ✓ Login User
                </li>
                <li class="text-muted ml-3 mb-2">
                  <code>POST /auth/verify-otp</code><br />
                  ✓ Verify Otp
                </li>
                <li class="text-muted ml-3 mb-2">
                  <code>POST /auth/resend-otp</code><br />
                  ✓ Resend Otp
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
